+			
+			else {
+				// 게임을 끝내면,
+				GameSystem.stopGame();
+				// GameSystem의 start = False
+				
+				
+				timeThread.interrupt();
+				// 시간 스레드 종료
+				
+			}
+		}		
+	}
+	
+}
