+		} else {
+			this.setIcon(null);
+		}
+	}
+	
+	
+	/*
+	 *  button click 관련 method
+	 */
+	
+	/* 버튼 image Change*/
+	public void showNumberImage() {
+
+		this.setWhite();
+
+		if (value == 0) {
+			this.setBlock();
+			this.setEnabled(false); 
+		}
+		
+		if (value != 0) {
